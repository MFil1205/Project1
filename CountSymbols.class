package CountSymbols;

import java.util.HashMap;
import java.util.Scanner;
import java.util.SortedMap;
import java.util.TreeMap;

public class CountSymbols {

	public static void main(String[] args) {
	
		Scanner scan = new Scanner(System.in);
		String input = scan.nextLine();
		int counter = 1;
		int lenght = input.length();
		
		TreeMap<Character, Integer> collection = new  TreeMap<Character, Integer>();
		
		for (int i = 0;i < lenght; i++){
		    
		    if(!collection.containsKey(input.charAt(i))){
		    	collection.put(input.charAt(i), counter);
		    }
		    else
		    	collection.replace((input.charAt(i)), counter + 1);
		    
		}
		for (Character name: collection.keySet()){

            String key =name.toString();
            String value = collection.get(name).toString();  
            System.out.println(key + ":" + " " + value + " time/s");  
		} 
	}

}
